 pipeline
	{
		agent any
			
				stages
					{
						stage('scm checkout')
						{
							steps{
								git branch: 'master', url: 'https://github.com/Ajeetsdhak/maven-project'
								}
							
						}	
						
						
						stage('install maven')
						{
							steps{
								WithMaven(jdk:'LocalJdk', maven: 'LocalMaven')
								}
							
						}
					}	
			
	}		
