 pipeline
	{
		agent any
			
				stages
					{
						stage('scm checkout')
						{
							steps{
								git branch: 'master', url: 'https://github.com/Ajeetsdhak/maven-project'
								}
							
						}	
						
						stage('sonar and maven')
						{
							steps{
								withSonarQubeEnv('sonar')
								{
								withMaven(jdk: 'LocalJdk', maven: 'LocalMaven')
								{
								sh 'mvn install sonar:sonar'
								}
								}
							}
						}
					}	
			
	}		
